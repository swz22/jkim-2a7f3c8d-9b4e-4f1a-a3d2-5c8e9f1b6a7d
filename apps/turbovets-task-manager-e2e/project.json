{
  "name": "turbovets-task-manager-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["turbovets-task-manager"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/turbovets-task-manager-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": [
        "turbovets-task-manager:build",
        "turbovets-task-manager:serve"
      ]
    }
  }
}
